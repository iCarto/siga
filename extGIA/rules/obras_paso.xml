<?xml version="1.0" encoding="UTF-8"?>
<LAYER alias="obras_paso">
	<TABLENAME>obras_paso</TABLENAME>
	<PRIMARYKEY>
		<PKELEMENT>id_obra_paso</PKELEMENT>
	</PRIMARYKEY>
	<RECORDSET>

		<FIELD>
			<FIELDNAME>id_obra_paso</FIELDNAME>
		</FIELD>

		<FIELD>
			<FIELDNAME>fecha_actualizacion</FIELDNAME>
			<VALIDATIONRULE>DATE</VALIDATIONRULE>
		</FIELD>

		<!-- LOCATION WIDGETS START -->
		<FIELD>
		<FIELDNAME>area_mantenimiento</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>area_mantenimiento</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
		</DOMAINREADER>
	</FIELD>

	<FIELD>
		<FIELDNAME>base_contratista</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>base_contratista</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_am</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
		</DOMAINREADER>
	</FIELD>

	<FIELD>
		<FIELDNAME>tramo</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>tramo</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
		</DOMAINREADER>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>tipo_via</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>tipo_via</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_tramo</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>nombre_via</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>nombre_via</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_tramo</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_tv</DRDBFOREIGNKEY>
				<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
			</DOMAINREADER>
			<ENABLEIF>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de descanso</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de servicio</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de mantenimiento</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Enlace</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Estación de peaje</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Intercambiador</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Túnel</VALUE>
				</CONDITION>
			</ENABLEIF>
		</FIELD>

		<FIELD>
			<FIELDNAME>pk</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>pk</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_tv</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_nv</DRDBFOREIGNKEY>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>ramal</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>ramal_obras</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>direccion</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>direccion</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_intercambiador</DRDBFOREIGNKEY>
			</DOMAINREADER>
			<ENABLEIF>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Intercambiador</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Enlace</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de Servicio</VALUE>
				</CONDITION>
			</ENABLEIF>
		</FIELD>

		<FIELD>
			<FIELDNAME>sentido</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>sentido</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>municipio</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>municipio</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<!-- LOCATION WIDGETS END -->

		<FIELD>
			<FIELDNAME>tipo_obra</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>tipo_obra_paso</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>tipologia</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>tipologia_obra_paso</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>utm_x</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>utm_y</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>utm_z</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>longitud</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>altura</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>anchura</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>luz_maxima</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>n_vanos</FIELDNAME>
			<VALIDATIONRULE>integer_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>n_pilas</FIELDNAME>
			<VALIDATIONRULE>integer_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>n_carriles</FIELDNAME>
			<VALIDATIONRULE>integer_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>galibo_h</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>galibo_v</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
		</FIELD>

	</RECORDSET>
</LAYER>