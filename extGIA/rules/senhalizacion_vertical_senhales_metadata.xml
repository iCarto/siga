			<DRDBCOLUMNVALUE>nombre_senhal</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>codigo_senhal</DRDBFOREIGNKEY>
		</DOMAINREADER>
		<ENABLEIF>
		<CONDITION>
		<COMPONENT>codigo_senhal</COMPONENT>
		<VALUE>Otro</VALUE>
		</CONDITION>
		</ENABLEIF>
	</FIELD>
	
