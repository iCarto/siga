	<FIELD>
		<FIELDNAME>tipo_senhal</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>tipo_senhal</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
		</DOMAINREADER>
	</FIELD>
	
	<FIELD>
