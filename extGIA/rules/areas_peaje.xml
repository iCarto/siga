<?xml version="1.0" encoding="UTF-8"?>
<LAYER alias="areas_peaje">
	<TABLENAME>areas_peaje</TABLENAME>
	<PRIMARYKEY>
		<PKELEMENT>id_area_peaje</PKELEMENT>
	</PRIMARYKEY>
	<RECORDSET>

		<FIELD>
			<FIELDNAME>id_area_peaje</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		</FIELD>

		<FIELD>
			<FIELDNAME>fecha_actualizacion</FIELDNAME>
			<VALIDATIONRULE>DATE</VALIDATIONRULE>
		</FIELD>

		<!-- LOCATION WIDGETS START -->
				<FIELD>
		<FIELDNAME>area_mantenimiento</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>area_mantenimiento</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
		</DOMAINREADER>
	</FIELD>

	<FIELD>
		<FIELDNAME>base_contratista</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>base_contratista</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_am</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
		</DOMAINREADER>
	</FIELD>

	<FIELD>
		<FIELDNAME>tramo</FIELDNAME>
		<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>tramo</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
		</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>tipo_via</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>tipo_via</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_tramo</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>nombre_via</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>nombre_via</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_tramo</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_tv</DRDBFOREIGNKEY>
				<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
			</DOMAINREADER>
			<ENABLEIF>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de descanso</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de servicio</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de mantenimiento</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Enlace</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Estación de peaje</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Intercambiador</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Túnel</VALUE>
				</CONDITION>
			</ENABLEIF>
		</FIELD>

		<FIELD>
			<FIELDNAME>pk</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>pk</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_tv</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_nv</DRDBFOREIGNKEY>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>sentido</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>sentido</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>municipio</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>municipio</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<!-- LOCATION WIDGETS END -->

		<FIELD>
			<FIELDNAME>fecha_puesta_servicio</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>

		<FIELD>
			<FIELDNAME>bordillos</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>bumpers</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>long_tunel</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
			<ENABLEIF>
				<CONDITION>
				<COMPONENT>tunel_peaje</COMPONENT>
				<VALUE>TRUE</VALUE>
				</CONDITION>
			</ENABLEIF>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>marquesina_tipo</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>areas_peaje_tipo</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>marquesina_sup</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>sup_total</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>

		<FIELD>
			<FIELDNAME>sup_trieff</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>sup_hormigon</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>sup_otros</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
		
		<FIELD>
			<FIELDNAME>numero_vias</FIELDNAME>
			<VALIDATIONRULE>INTEGER_POSITIVE</VALIDATIONRULE>
		</FIELD>
	</RECORDSET>
</LAYER>