<RECORDSET>

	<FIELD>
		<FIELDNAME>tipo_consulta</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>tipo_consulta</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
		</DOMAINREADER>
	</FIELD>
	
	<FIELD>
		<FIELDNAME>elemento</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>elemento</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_tipo_consulta</DRDBFOREIGNKEY>
		</DOMAINREADER>
	</FIELD>

	

	<!-- LOCATION WIDGETS START -->
	<FIELD>
		<FIELDNAME>area_mantenimiento</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>area_mantenimiento</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
		</DOMAINREADER>
	</FIELD>

	<FIELD>
		<FIELDNAME>base_contratista</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>base_contratista</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_am</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
		</DOMAINREADER>
	</FIELD>

	<FIELD>
		<FIELDNAME>tramo</FIELDNAME>
		<DOMAINREADER>
			<DRTYPE>db</DRTYPE>
			<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
			<DRDBTABLE>tramo</DRDBTABLE>
			<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
			<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			<DRDBFOREIGNKEY>id_bc</DRDBFOREIGNKEY>
			<DRADDVOIDVALUE>true</DRADDVOIDVALUE>
		</DOMAINREADER>
	</FIELD>
	<!-- LOCATION WIDGETS END -->
	
	<FIELD>
		<FIELDNAME>fecha_inicio</FIELDNAME>
		<!-- a trick no make it noneditable at the beggining-->
		<NONEDITABLE></NONEDITABLE>
		<ENABLEIF>
			<CONDITION>
			<COMPONENT>tipo_consulta</COMPONENT>
			<VALUE>Trabajos</VALUE>
			</CONDITION>
			<CONDITION>
			<COMPONENT>tipo_consulta</COMPONENT>
			<VALUE>Inspecciones</VALUE>
			</CONDITION>
			<CONDITION>
			<COMPONENT>tipo_consulta</COMPONENT>
			<VALUE>Trabajos Agrupados</VALUE>
			</CONDITION>
		</ENABLEIF>
	</FIELD>

	<FIELD>
		<FIELDNAME>fecha_fin</FIELDNAME>
		<!-- a trick no make it noneditable at the beggining-->
		<NONEDITABLE></NONEDITABLE>
		<ENABLEIF>
			<CONDITION>
			<COMPONENT>tipo_consulta</COMPONENT>
			<VALUE>Trabajos</VALUE>
			</CONDITION>
			<CONDITION>
			<COMPONENT>tipo_consulta</COMPONENT>
			<VALUE>Inspecciones</VALUE>
			</CONDITION>
			<CONDITION>
			<COMPONENT>tipo_consulta</COMPONENT>
			<VALUE>Trabajos Agrupados</VALUE>
			</CONDITION>
		</ENABLEIF>
	</FIELD>
</RECORDSET>