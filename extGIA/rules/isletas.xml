			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>pk</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_tv</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_nv</DRDBFOREIGNKEY>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>pk_final</FIELDNAME>
			<VALIDATIONRULE>double_positive</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>pk</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_tv</DRDBFOREIGNKEY>
				<DRDBFOREIGNKEY>id_nv</DRDBFOREIGNKEY>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>ramal</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>ramal</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>direccion</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>direccion</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
				<DRDBFOREIGNKEY>id_intercambiador</DRDBFOREIGNKEY>
			</DOMAINREADER>
			<ENABLEIF>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Intercambiador</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Enlace</VALUE>
				</CONDITION>
				<CONDITION>
				<COMPONENT>tipo_via</COMPONENT>
				<VALUE>Área de Servicio</VALUE>
				</CONDITION>
			</ENABLEIF>
		</FIELD>

		<FIELD>
			<FIELDNAME>sentido</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>sentido</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>margen</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>margen</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>item</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<FIELD>
			<FIELDNAME>municipio</FIELDNAME>
			<VALIDATIONRULE>MANDATORY</VALIDATIONRULE>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>municipio</DRDBTABLE>
				<DRDBCOLUMNALIAS>item</DRDBCOLUMNALIAS>
				<DRDBCOLUMNVALUE>id</DRDBCOLUMNVALUE>
			</DOMAINREADER>
		</FIELD>

		<!-- LOCATION WIDGETS END -->

		<FIELD>
			<FIELDNAME>tipo_isleta</FIELDNAME>
			<DOMAINREADER>
				<DRTYPE>db</DRTYPE>
				<DRDBSCHEMA>audasa_extgia_dominios</DRDBSCHEMA>
				<DRDBTABLE>tipo_isleta</DRDBTABLE>
