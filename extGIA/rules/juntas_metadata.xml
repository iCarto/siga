<?xml version="1.0" encoding="UTF-8"?>
<LAYER alias="juntas">
	<TABLENAME>juntas</TABLENAME>
	<PRIMARYKEY>
		<PKELEMENT>id_junta</PKELEMENT>
	</PRIMARYKEY>
	<RECORDSET>

		<FIELD>
			<FIELDNAME>id_junta</FIELDNAME>
