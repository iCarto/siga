<assembly>
	<id>distribution</id>
	<formats>
		<format>dir</format>
	</formats>
	<fileSets>
	<!-- Extructure for the extension -->
		<fileSet>
			<directory>config</directory>
			<outputDirectory>${extension-distribution}
			</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>images</directory>
			<outputDirectory>/${extension-distribution}/images
			</outputDirectory>
		</fileSet>
		<fileSet>
			<directory>northimages</directory>
			<outputDirectory>/${extension-distribution}/northimages
			</outputDirectory>
		</fileSet>
	</fileSets>
	<files>
		<file>
			<source>build.number</source>
			<outputDirectory>${extension-distribution}
			</outputDirectory>
		</file>
	</files>
